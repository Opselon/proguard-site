{
    "name": "proguard-site-worker",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "node build.mjs",
        "dev": "npm run build && wrangler dev src/index.mjs --local --port 8788",
        "deploy": "npm run build && wrangler deploy src/index.mjs"
    },
    "devDependencies": {
        "@playwright/test": "^1.56.1",
        "fs-extra": "^11.3.2",
        "wrangler": "^3.114.15"
